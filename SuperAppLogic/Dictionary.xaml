<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local_vm="clr-namespace:SuperAppLogic.ViewModels;assembly=SuperAppLogic"
                    xmlns:local_views="clr-namespace:SuperAppLogic.Views;assembly=SuperAppLogic">
    <DataTemplate DataType="{x:Type local_vm:FirstViewModel}">
        <local_views:FirstView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type local_vm:SecondViewModel}">
        <local_views:SecondView/>
    </DataTemplate>
</ResourceDictionary>